                            <circle cx="3.5" cy="5.5" r=".5" />
                            <circle cx="3.5" cy="8" r=".5" />
                            <circle cx="3.5" cy="10.5" r=".5" />
                        </svg>
                        Jobs List
                    </a>
                </li>
                <li class="nav-item active">
                    <a id="Varieties_Offers" class="nav-link" href="~/Categories/Index">
                        <i class="bi bi-layout-text-window"></i>
                        Categories
                    </a>
                </li>
                <li class="nav-item active">
                    <a id="Contact_" class="nav-link" href="~/Home/Contact">
                        <svg xmlns="http://www.w3.org/2000/svg" width="22" height="22" fill="currentColor" class="bi bi-envelope-open" viewBox="0 0 16 16">
                            <path fill-rule="evenodd" d="M8.47 1.318a1 1 0 0 0-.94 0l-6 3.2A1 1 0 0 0 1 5.4v.818l5.724 3.465L8 8.917l1.276.766L15 6.218V5.4a1 1 0 0 0-.53-.882l-6-3.2zM15 7.388l-4.754 2.877L15 13.117v-5.73zm-.035 6.874L8 10.083l-6.965 4.18A1 1 0 0 0 2 15h12a1 1 0 0 0 .965-.738zM1 13.117l4.754-2.852L1 7.387v5.73zM7.059.435a2 2 0 0 1 1.882 0l6 3.2A2 2 0 0 1 16 5.4V14a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V5.4a2 2 0 0 1 1.059-1.765l6-3.2z" />
                        </svg>
                        Contact Us
                    </a>
                </li>
                <li class="nav-item active">
                    <a id="About_" class="nav-link" href="#">
                        <svg xmlns="http://www.w3.org/2000/svg" width="22" height="22" fill="currentColor" class="bi bi-exclamation-square" viewBox="0 0 16 16">
                            <path fill-rule="evenodd" d="M14 1H2a1 1 0 0 0-1 1v12a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1V2a1 1 0 0 0-1-1zM2 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2H2z" />
                            <path d="M7.002 11a1 1 0 1 1 2 0 1 1 0 0 1-2 0zM7.1 4.995a.905.905 0 1 1 1.8 0l-.35 3.507a.552.552 0 0 1-1.1 0L7.1 4.995z" />
                        </svg>
                        About
                    </a>
                </li>

                <li class="nav-item active" style="margin-left:50px;">
                    @using (Html.BeginForm("Search", "Home", FormMethod.Post))
                    {
                        @*<i class="bi bi-search"></i>*@
                        <input type="text" name="SearchName" id="SearchName"
                               placeholder="Search ..." />
                    }



                </li>


            </ul>

            <!--Partial _Login -->
           @Html.Partial("_Login")
        </div>
    </nav>


    <div class="container" style="min-height:450px; padding-top:90px;">
        <div class="row">
            @RenderBody()
        </div>
    </div>
    <!--footer-->
    <!-- Footer -->
    <footer class="mainfooter" role="contentinfo">
        <div class="footer-middle">
            <div class="container">
                <div class="row">
                    <div class="col-md-4 col-sm-6">
                        <!--Column1-->
                        <div class="footer-pad">
                            <h4>Heading 1</h4>
                            <ul class="list-unstyled">
                                <li><a href="#"></a></li>
                                <li><a href="#">Payment Center</a></li>
                                <li><a href="#">Contact Directory</a></li>
                                <li><a href="#">Forms</a></li>
                                <li><a href="#">News and Updates</a></li>
                            </ul>
